{
  "name": "primedian",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.13.0",
    "yarn": "1.12.3"
  },
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4"
  },
  "scripts": {
    "build": "node scripts build",
    "dev": "node scripts start",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint --fix",
    "prod": "node scripts prod",
    "start": "node src/index.js"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "client/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
