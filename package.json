{
  "name": "primedian",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.13.0",
    "yarn": "1.12.3"
  },
  "scripts": {
    "dev": "express-scripts start",
    "eject": "express-scripts eject",
    "format": "prettier --write \"src/**/*.js\"",
    "install:client": "yarn --cwd client install",
    "lint": "eslint --fix",
    "postinstall": "yarn run install:client",
    "start": "node src/index.js"
  },
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@cszatma/express-scripts": "file:/Users/christopherszatmary/Developer/TypeScript/Projects/create-express-app/packages/@cszatma/express-scripts",
    "concurrently": "^4.1.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "client/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
